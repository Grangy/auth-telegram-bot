# Server Configuration
PORT=3000

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
BOT_USERNAME=your_bot_username

# Database Configuration (MongoDB Atlas)
# Получите URL подключения в MongoDB Atlas: https://cloud.mongodb.com
DATABASE_URL="mongodb+srv://your-username:your-password@your-cluster.mongodb.net/telegram-auth?retryWrites=true&w=majority"

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Logging Configuration
LOG_LEVEL=info
